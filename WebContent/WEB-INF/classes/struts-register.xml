"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="register" namespace="/register" extends="default">
		<result-types>
			<result-type name="tiles"
				class="org.apache.struts2.views.tiles.TilesResult" />
		</result-types>

		<action name="registerLink" method="register"
			class="com.ibm.opum.register.action.RegisterAction">
			<result name="register" type="tiles">registerLink</result>
		</action>
	</package>
</struts>